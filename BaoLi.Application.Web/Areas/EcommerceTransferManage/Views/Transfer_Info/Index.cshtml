@{;
ViewBag.Title = "资金划拨";
Layout = "~/Views/Shared/_Index.cshtml";
}
<script src="~/Areas/EcommerceTransferManage/Content/Script/EcommerceTransfer.js"></script>
<div class="titlePanel">
    <div class="title-search">
        <table class="searchTable">
            <tr>
                <th class="formTitle">电商简称:</th>
                <td>
                    <input id="C_Name" type="text" class="form-control" placeholder="请输入要查询关键字" style="width: 200px;" />
                </td>
                <th class="formTitle">区域公司:</th>
                <td class="formValue">
                    <div id="Project_Id" type="select" class="ui-select">
                    </div>
                </td>
                <th class="formTitle">项目名称:</th>
                <td>
                    <div id="Project_Name" type="select" class="ui-select">
                    </div>
                </td>
            </tr>
            <tr>
                <th class="formTitle">划拨发起时间:<font face="宋体"></font></th>
                <td class="formValue">
                    <input id="Transfer_Createtime" type="text" class="form-control input-wdatepicker" onfocus="WdatePicker()" />
                </td>
                <th class="formTitle">至<font face="宋体"></font></th>
                <td class="formValue">
                    <input id="Transfer_Endtime" type="text" class="form-control input-wdatepicker" onfocus="WdatePicker()" />
                <th class="formTitle"></th>
                <td style="padding-left: 5px;">
                    <a id="btn_Search" class="btn btn-primary"><i class="fa fa-search"></i> 查询</a>
                </td>
            </tr>
        </table>
    </div>
    <div class="toolbar">
        <div class="btn-group">
            <a id="lr-replace" class="btn btn-default" onclick="reload()"><i class="fa fa-refresh"></i>刷新</a>
            <a id="lr-add" class="btn btn-default" onclick="btn_add()"><i class="fa fa-plus"></i>新增</a>
            <a id="lr-detail" class="btn btn-default" onclick="btn_detail()"><i class="fa fa-pencil-square-o"></i>详细</a>
            <a id="lr-delete" class="btn btn-default" onclick="btn_delete()"><i class="fa fa-trash-o"></i>删除</a>
        </div>
        <script>$('.toolbar').authorizeButton()</script>
    </div>

</div>
<div class="gridPanel">
    <table id="gridTable"></table>
    <div id="gridPager"></div>
</div>
